{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Rebuild",
            "type": "cppdbg",
            "request": "launch",
            "preLaunchTask": "CMake: clean rebuild",
            "postDebugTask": "",
            "program": "${workspaceFolder}/build/src/EulerSYCL",
            "args": [
                "${workspaceFolder}/setup.ini",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/opt/intel/oneapi/debugger/2023.0.0/gdb/intel64/bin/gdb-oneapi",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Disable target async",
                    "text": "set target-async off",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Usually Run",
            "type": "cppdbg",
            "request": "launch",
            "preLaunchTask": "",
            "postDebugTask": "",
            "program": "${workspaceFolder}/build/src/EulerSYCL",
            "args": [
                "${workspaceFolder}/setup.ini",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/opt/intel/oneapi/debugger/2023.0.0/gdb/intel64/bin/gdb-oneapi",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Disable target async",
                    "text": "set target-async off",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}